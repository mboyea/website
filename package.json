{
	"name": "www-mboyea-com",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "pnpm i && pnpm run build && pnpm run deploy:secrets && pnpm run deploy:app",
		"deploy:secrets": "bash scripts/stage_secrets.sh",
		"deploy:app": "flyctl deploy -a www-mboyea-com",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.20.0",
		"sass": "^1.63.4",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.2",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	}
}
